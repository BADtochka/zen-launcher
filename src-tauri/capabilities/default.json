{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Full FS access",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:read-all",
    "fs:write-all",
    {
      "identifier": "fs:scope",
      "allow": [
        "**/*"
      ]
    },
"shell:default"
  ]
}